{
  "name": "kielbasa",
  "version": "0.5.0",
  "description": "All-Natural Casing Utilities in JavaScript",
  "main": "dist/kielbasa.js",
  "scripts": {
    "build": "npm run build:bundle && npm run build:cli",
    "build:cli": "babel src/cli.js -o dist/cli.js",
    "build:bundle": "webpack --config webpack.config.js",
    "precommit": "lint-staged",
    "start": "npm run build:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": "dist/cli.js",
  "unpkg": "dist/kielbasa.min.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/joshingmachine/kielbasa"
  },
  "keywords": [
    "camel",
    "case",
    "convention",
    "kebab",
    "lower",
    "naming",
    "notation",
    "pascal",
    "snake",
    "start",
    "upper"
  ],
  "files": [
    "/dist"
  ],
  "author": "Josh Smith",
  "license": "MIT",
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.2",
    "eslint": "^4.18.2",
    "eslint-config-joshingmachine": "^0.6.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.11.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "commander": "^2.17.1"
  }
}
